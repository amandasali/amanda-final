{% extends '_layouts/base.njk' %}

{% block content %}
<article class="container">
  <div class="row">
    <div class="col-sm-4 left-rail">
      {% block info %} 
      <img class="img-fluid" src="assets/img/img/img/members/{{members.image}}">
       {% endblock %}
    </div>
    <div class="col-sm-8">
      {% block story %}
      <h1>{{ members.name }}</h1>
      <p>{{ members.text | safe }}</p>
      {% endblock %}


    </div>
  </div>
</article>
{% endblock %}
